{
  "mcpServers": {
    "secure-test": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/test-server/minimal-test-server.js"
      ]
    },
    "image-gen": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/cookbook/image-gen-server/dist/index.js"
      ],
      "env": {
        "BFL_API_KEY": "your_bfl_api_key",
        "OPENAI_API_KEY": "your_openai_api_key",
        "STABILITY_API_KEY": "your_stability_api_key",
        "IDEOGRAM_API_KEY": "your_ideogram_api_key",
        "GOOGLE_GENAI_API_KEY": "your_google_api_key"
      }
    },
    "kenpom": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/cookbook/kenpom-server/dist/index.js"
      ],
      "env": {
        "KENPOM_EMAIL": "your_kenpom_email",
        "KENPOM_PASSWORD": "your_kenpom_password"
      }
    },
    "nba": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/cookbook/nba-server/dist/index.js"
      ]
    },
    "api-wrapper": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/cookbook/api-wrapper-server/dist/index.js"
      ]
    },
    "filesystem": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/cookbook/filesystem-server/dist/index.js"
      ]
    },
    "database": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/path/to/mcp-security/cookbook/database-server/dist/index.js"
      ]
    }
  }
}
